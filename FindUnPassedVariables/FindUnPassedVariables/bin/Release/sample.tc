<?xml version="1.0" encoding="utf-8"?>
<testcases version="2.0">

  <testcase name ="SetUp_DateRange" desc ="SetUp_DateRange">
    <filelog>
      <author>Naresh B</author>
    </filelog>
    <identifiers>
      <include path="/testcases/_common/identifiers.id"/>
    </identifiers>
    <data>
      <var name ="@CSCOnsole_ClientID"/>
      <var name ="@CSCOnsole_UserName"/>
      <var name ="@CSCOnsole_Password"/>
      <var name ="@CSConsole_URL"/>
      <var name ="@FilePath"/>
      <var name ="@StartPayrollDate"/>
      <var name ="@EndPayrollDate"/> 
      <var name ="@EmployeeID"/>
      <var name ="@Yes"/>
    </data>
    <basestate name ="CSconsole Home Basestate">
      <dataset ref="context" >
        <map var="@CSCOnsole_ClientID" value="@CSCOnsole_ClientID"/>
        <map var="@CSCOnsole_UserName" value="@CSCOnsole_UserName"/>
        <map var="@CSCOnsole_Password" value="@CSCOnsole_Password"/>
        <map var="@CSConsole_URL" value="@CSConsole_URL"/>
      </dataset>
    </basestate>
    <ops>
      <navigate path="HRRefresh"/>
      <call name="HR Refresh import file" path="/testcases/APIs/csconsole/HRRefresh/HRRefresh.tc">
        <dataset ref="context">
          <map var ="@FilePath" value="@FilePath"/>
          <map var ="@Import" value="@Yes"/>
          <map var ="@Back" value="@Yes"/>
          <map var ="@Console" value ="@Yes"/>
        </dataset>
      </call>
      <call name="Run SOD" path="/testcases/APIs/csconsole/StartOfDay/StartOfDay.tc">
        <dataset ref="context">
          <map var="@NavigatePath" value="StartofDay"/>
          <map var="@StartPayrollDate" value="@StartPayrollDate"/>
          <map var="@EndPayrollDate" value="@EndPayrollDate"/>
          <map var="@EmployeeID" value ="@EmployeeID"/>
          <map var="@Console" value="@Yes"/>
        </dataset>
      </call>
    </ops>
  </testcase>

  <testcase qcid="49527" name ="PayrollShiftDetails_Section_1" desc =": Verify if the [Scheduled Shift] droplist is displayed in [Payroll Shift Details] section, when [Preferences]>> [Payroll Prefs] >> [Payroll Shift Detail ]>> [Show Scheduled Shift] is checked">
    <filelog>
      <author>Naresh B</author>
    </filelog>
    <identifiers>
      <include path ="/testcases/APIs/RIA/TimeAndAttendance/TimeInDetail/TimeInDetail.id"/>
    </identifiers>
    <data>
      <var name ="@RIA_ClientID"/>
      <var name ="@User_FirstName"/>
      <var name ="@RIA_UserPassword"/>
      <var name ="@RIA_URL"/>

      <var name ="@PrefBarName"/>
      <var name ="@CheckAnyCheckBox"/>
      <var name ="@Yes"/>

      <var name ="@WeekOf"/>
      <var name ="@WeekDay"/>
      <var name ="@Date"/>
      <var name ="@ShiftType"/>
      <var name ="@Yes"/>
    </data>
    <ops>
      <call name="RIABasestate" path="/testcases/_common/common.tc">
        <dataset ref="context">
          <map var="@RIA_ClientID" value="@RIA_ClientID"/>
          <map var="@RIA_UserID" value="@User_FirstName"/>
          <map var="@RIA_Password" value="@RIA_UserPassword"/>
          <map var="@RIA_URL" value="@RIA_URL"/>
          <map var="@NavPath" value ="Admin_ScreenPreferences"/>
        </dataset>
      </call>

      <call name ="SelectingScreenPreferences" path ="/testcases/APIs/RIA/Admin/ScreenPreferences/ScreenPreferences.tc">
        <dataset ref ="context">
          <map var ="@PrefBarName" value ="@PrefBarName"/>
          <map var ="@CheckAnyCheckBox" value ="@CheckAnyCheckBox"/>
          <map var ="@LocalizeSave" value ="@Yes"/>
        </dataset>
      </call>

      <navigate path="TimeandAttendance_TimeInDetail"/>

      <call name="FilterEmployeeDetails" path="/testcases/APIs/_APIUtility/_APIUtility.tc">
        <dataset ref="context">
          <map var="@WeekOf" value="@WeekOf"/>
        </dataset>
      </call>
      <waitforcontrolload controlid ="TimeAndAttendance_TimeInDetail_txtScheduledShift" controlidreplacements ="@WeekDay,@Date,@ShiftType"/>
      <assert>      
        <isexists locator="TimeAndAttendance_TimeInDetail_txtScheduledShift" locatorreplacements="@WeekDay,@Date,@ShiftType"/>
      </assert>
    </ops>
  </testcase>

